<form method="post" action="/movie/add">
	<label>Movie Name</label><br>
	<input type="text" name="title">
	<br>
	<label>Year</label><br>
	<input type="text" name="year">
	<br>
	<input type="submit" value="submit">
</form>

<h3>Movies</h3>

<ul>
<% movies.forEach(function(movie) { %>
	<li><%= movie.title %></li>
<% }) %>
</ul>

<h3>Persons</h3>
<ul>
<% persons.forEach(function(person) { %>
	<li><%= person.name %></li>
<% }) %>
</ul>